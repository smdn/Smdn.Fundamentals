<Project>
  <PropertyGroup>
    <!-- <_SmdnProjectAssets_ImportFromLocal Condition="'$(_SmdnProjectAssets_ImportFromLocal)' == ''">true</_SmdnProjectAssets_ImportFromLocal> -->
  </PropertyGroup>

  <ImportGroup Label="project assets">
    <Import Project="$(MSBuildThisFileDirectory)..\src\ProjectAssets.props" />
  </ImportGroup>

  <PropertyGroup>
    <Nullable>warnings</Nullable>

    <TestTargetLibraryProjectDirectory
      Condition="'$(TestTargetLibraryProjectName)' != ''"
    >$(MSBuildThisFileDirectory)..\src\$(TestTargetLibraryProjectName)\</TestTargetLibraryProjectDirectory>
  </PropertyGroup>

  <ItemGroup Condition=" '$(TestTargetLibraryProjectName)' != '' ">
    <PackageReference Include="NUnit" Version="3.13.2" />
    <PackageReference Include="NUnit3TestAdapter" Version="4.2.1" />
    <PackageReference Include="Microsoft.NET.Test.Sdk" Version="17.1.0"/>
  </ItemGroup>

  <ItemGroup>
    <PackageReference
      Include="Smdn.MSBuild.DefineConstants.NETSdkApi"
      Version="[1.3.5]"
      PrivateAssets="all"
      IncludeAssets="build"
    />
  </ItemGroup>
</Project>
