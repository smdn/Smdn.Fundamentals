<Project>
  <PropertyGroup>
    <!-- default configurations -->
    <EnableImportingNETSdkApiDefineConstantsProps
      Condition=" '$(EnableImportingNETSdkApiDefineConstantsProps)' == '' "
    >true</EnableImportingNETSdkApiDefineConstantsProps>
  </PropertyGroup>

  <Import
    Project="$(MSBuildThisFileDirectory)project\define-constants\DefineConstants.NETSdkApi.props"
    Condition="'$(EnableImportingNETSdkApiDefineConstantsProps)' == 'true'"
  />
</Project>
