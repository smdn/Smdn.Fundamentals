      Include="..\Smdn.Fundamental.Stream\Smdn.Fundamental.Stream.csproj"
      Condition="
        !$(TargetFramework.StartsWith('net5')) or
        !$(TargetFramework.StartsWith('net48')) or
        !$(TargetFramework.StartsWith('net472')) or
        !$(TargetFramework.StartsWith('netstandard2'))
      "
    />
