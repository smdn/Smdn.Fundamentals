    <VersionSuffix></VersionSuffix>
  </PropertyGroup>

  <PropertyGroup Label="metadata">
    <Title>Smdn.Fundamental.PrintableEncoding.QuotedPrintable</Title>
    <Description>Smdn.Fundamental.PrintableEncoding.QuotedPrintable</Description>
    <CopyrightYear>2021</CopyrightYear>

    <!-- NuGet -->
    <!--<PackageTags></PackageTags>-->
