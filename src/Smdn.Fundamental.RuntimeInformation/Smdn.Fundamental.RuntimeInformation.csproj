<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net6.0;net47;net45;netstandard2.0;netstandard1.6;netstandard1.5;netstandard1.3;netstandard1.1</TargetFrameworks>
    <RootNamespace>Smdn</RootNamespace>
    <VersionPrefix>3.0.2</VersionPrefix>
    <VersionSuffix></VersionSuffix>
    <!-- <PackageValidationBaselineVersion>3.0.0</PackageValidationBaselineVersion> -->
    <Nullable>enable</Nullable>
    <EnableTrimAnalyzer>false</EnableTrimAnalyzer>
    <RootNamespace/> <!-- empty the root namespace so that the namespace is determined only by the directory name, for code style rule IDE0030 -->
  </PropertyGroup>

  <PropertyGroup Label="assembly attributes">
    <Description>A utility library for retrieving .NET runtime environment information.</Description>
    <CopyrightYear>2022</CopyrightYear>
  </PropertyGroup>

  <PropertyGroup Label="package properties">
    <PackageTags>runtime;utility</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Runtime.InteropServices.RuntimeInformation" />
  </ItemGroup>
</Project>
